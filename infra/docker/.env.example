DJANGO_ENV=development
DJANGO_DEBUG=true
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1,backend

DB_HOST=mariadb
DB_PORT=3306
DB_NAME=riskfabric
DB_USER=riskfabric
DB_PASSWORD=riskfabric
MARIADB_ROOT_PASSWORD=riskfabric_root

CELERY_BROKER_URL=redis://redis:6379/1
CELERY_RESULT_BACKEND=redis://redis:6379/2

EAM_PLUGIN_NAME=excel_bootstrap
EAM_PLUGIN_VERSION=v1
EAM_EXCEL_FILE_PATH=/datasource/inventory.xls

AUDIT_RETENTION_DAYS=180
AUDIT_RETENTION_SCHEDULE_HOUR=2
AUDIT_RETENTION_SCHEDULE_MINUTE=30
